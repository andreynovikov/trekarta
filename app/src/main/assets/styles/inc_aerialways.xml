<?xml version="1.0" encoding="UTF-8"?>
<rendertheme version="1" xmlns="http://opensciencemap.org/rendertheme">
    <m cat="aerialways" k="aerialway">
        <m closed="no" e="way">
            <line cap="butt" fix="true" stroke="#202020" width="1.8" />
            <m v="cable_car|gondola">
                <line fix="true" src="assets:symbols/dot.svg" repeat-start="10" repeat-gap="10"
                    symbol-color="#202020" symbol-height="12" />
                <m zoom-min="14">
                    <text style="bold" fill="#606060" k="name" size="16" stroke="#ffffff"
                        stroke-width="2.0" />
                </m>
            </m>
            <m v="chair_lift|drag_lift">
                <line fix="true" src="assets:symbols/tick.svg" symbol-color="#202020" repeat-start="10" repeat-gap="10"
                    symbol-height="12" />
            </m>
            <m v="zip_line" zoom-min="15">
                <symbol cat="hiking" merge-gap="50" merge-group="aerialway-symbols" repeat="true"
                    repeat-gap="50" src="assets:symbols/transport/zipline.svg" text-overlap="false" />
            </m>
        </m>
        <m e="node" v="station">
            <symbol src="assets:symbols/transport/station.svg" />
            <text caption="true" style="bold" dy="-10" fill="#ec2d2d" k="name" size="14" stroke="#ffffff"
                stroke-width="2.0" />
        </m>
    </m>
</rendertheme>
<?xml version="1.0" encoding="UTF-8"?>
<rendertheme version="1" xmlns="http://opensciencemap.org/rendertheme">
    <!-- Hiking ways -->
    <m cat="hiking" e="way" k="highway" zoom-min="14" select="first">
        <!-- No-access paths -->
        <m v="cycleway">
            <m k="foot" v="-|yes|designated">
                <line fix="true" stroke="#ffffff" width="3.0" />
                <line use="path" stipple-stroke="color:pedestrian" width="0.9" />
            </m>
        </m>
        <m k="foot" v="no|private">
            <line fix="true" stroke="#ffffff" width="3.0" />
            <line use="path" stipple-stroke="color:pedestrian" width="0.9" />
        </m>
        <m k="foot" v="~">
            <m k="access" v="no|private">
                <line fix="true" stroke="#ffffff" width="3.0" />
                <line use="path" stipple-stroke="color:pedestrian" width="0.9" />
            </m>
        </m>
        <!-- Available paths -->
        <m>
            <m v="path|footway">
                <m k="bridge" v="~|no">
                    <!-- TODO Background line is put here because of line style ordering error in legend view -->
                    <line fix="true" stroke="#ffffff" width="3.0" />
                </m>
            </m>
            <m v="path|footway">
                <m k="bridge" v="~|no">
                    <m k="sac_scale" v="~">
                        <m k="trail_visibility" v="~">
                            <line fix="true" stipple="3" stipple-stroke="#aab28f67" stroke="color:transparent"
                                width="2.3" />
                        </m>
                        <m k="trail_visibility" v="excellent">
                            <line fix="true" stroke="#aab28f67" width="2.3" />
                        </m>
                        <m k="trail_visibility" v="good">
                            <line fix="true" stipple="5" stipple-ratio="0.8" stipple-stroke="#aab28f67"
                                stroke="color:transparent" width="2.3" />
                        </m>
                        <m k="trail_visibility" v="intermediate">
                            <line fix="true" stipple="5" stipple-ratio="0.6" stipple-stroke="#aab28f67"
                                stroke="color:transparent" width="2.3" />
                        </m>
                        <m k="trail_visibility" v="bad|horrible">
                            <line fix="true" stipple="5" stipple-ratio="0.4" stipple-stroke="#aab28f67"
                                stroke="color:transparent" width="2.3" />
                        </m>
                        <m k="trail_visibility" v="no">
                            <line fix="true" stipple="5" stipple-ratio="0.2" stipple-stroke="#aab28f67"
                                stroke="color:transparent" width="2.3" />
                        </m>
                    </m>
                </m>
            </m>
            <m v="cycleway">
                <m k="bridge" v="~|no">
                    <m k="foot" v="yes|designated">
                        <m k="sac_scale" v="~">
                            <line fix="true" stroke="#ffffff" width="3.0" />
                            <m k="trail_visibility" v="~">
                                <line fix="true" stipple="3" stipple-stroke="#aab28f67"
                                    stroke="color:transparent" width="2.3" />
                            </m>
                            <m k="trail_visibility" v="excellent">
                                <line fix="true" stroke="#aab28f67" width="2.3" />
                            </m>
                            <m k="trail_visibility" v="good">
                                <line fix="true" stipple="5" stipple-ratio="0.8"
                                    stipple-stroke="#aab28f67" stroke="color:transparent" width="2.3" />
                            </m>
                            <m k="trail_visibility" v="intermediate">
                                <line fix="true" stipple="5" stipple-ratio="0.6"
                                    stipple-stroke="#aab28f67" stroke="color:transparent" width="2.3" />
                            </m>
                            <m k="trail_visibility" v="bad|horrible">
                                <line fix="true" stipple="5" stipple-ratio="0.4"
                                    stipple-stroke="#aab28f67" stroke="color:transparent" width="2.3" />
                            </m>
                            <m k="trail_visibility" v="no">
                                <line fix="true" stipple="5" stipple-ratio="0.2"
                                    stipple-stroke="#aab28f67" stroke="color:transparent" width="2.3" />
                            </m>
                        </m>
                    </m>
                </m>
            </m>
